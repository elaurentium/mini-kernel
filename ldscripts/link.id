OUTPUT_FORMAT(elf32-littlearm)   /* Formato ELF para ARM */
ENTRY(start)                      /* Ponto de entrada */
SECTIONS
{
   . = 0x10000;                    /* Endereço inicial para o código (ajustado para ARM) */
   
   .text : 
   {
      *(.text)                     /* Seção de código */
   }
   
   .data : 
   {
      *(.data)                     /* Seção de dados */
   }
   
   .bss : 
   {
      *(.bss)                       /* Seção de dados não inicializados */
   }
}
